{
  "expo": {
    "name": "pōcher",
    "slug": "pōcher",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "po-cher",
    "splash": {
      "image": "./assets/pocher-label-b.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/pocher-label-b.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/pocher-label-w.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
